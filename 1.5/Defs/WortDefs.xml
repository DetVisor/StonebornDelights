  <ThingDef ParentName="ResourceBase">
    <defName>SD_ElixirMush</defName>
    <label>elixir mush</label>
    <description>Un-fermented elixir, with a very pungent odor. This substance needs to ferment in a fermenting barrel before it becomes drinkable beer.</description>
    <graphicData>
      <texPath>Things/ElixirMush</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <healthAffectsPrice>false</healthAffectsPrice>
    <rotatable>false</rotatable>
    <stackLimit>25</stackLimit>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
    <statBases>
      <DeteriorationRate>1.0</DeteriorationRate>
      <MarketValue>3</MarketValue>
      <Mass>0.8</Mass>
      <Flammability>0.6</Flammability>
      <MaxHitPoints>60</MaxHitPoints>
    </statBases>
    <costList>
        <SD_RawBullHorns>20</SD_RawBullHorns>
        <SD_RawBladeroot>10</SD_RawBladeroot>
      </costList>
    <recipeMaker>
        <workSpeedStat>DrugCookingSpeed</workSpeedStat>
        <workSkill>Cooking</workSkill>
        <recipeUsers>
            <li>Brewery</li>
        </recipeUsers>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_Brewing</soundWorking>
        <workAmount>1200</workAmount>
        <targetCountAdjustment>5</targetCountAdjustment>
    </recipeMaker>
    <tickerType>Rare</tickerType>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>8</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef>

  <ThingDef ParentName="ResourceBase">
    <defName>SD_LiquorSmush</defName>
    <label>liquor smush</label>
    <description>Un-fermented liquor, with a strong and spicy odor. This substance needs to ferment in a fermenting barrel before it becomes drinkable beer.</description>
    <graphicData>
      <texPath>Things/LiquorSmush</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <healthAffectsPrice>false</healthAffectsPrice>
    <rotatable>false</rotatable>
    <stackLimit>25</stackLimit>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
    <statBases>
      <DeteriorationRate>1.0</DeteriorationRate>
      <MarketValue>3</MarketValue>
      <Mass>1.1</Mass>
      <Flammability>0</Flammability>
      <MaxHitPoints>60</MaxHitPoints>
    </statBases>
    <costList>
        <SD_RawBullHorns>20</SD_RawBullHorns>
        <SD_RawBladeroot>10</SD_RawBladeroot>
      </costList>
    <recipeMaker>
        <workSpeedStat>DrugCookingSpeed</workSpeedStat>
        <workSkill>Cooking</workSkill>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_Brewing</soundWorking>
        <workAmount>1200</workAmount>
        <targetCountAdjustment>5</targetCountAdjustment>
    </recipeMaker>
    <tickerType>Rare</tickerType>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>8</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef>

  <RecipeDef>
    <defName>SD_MakeLiquorSmush</defName>
    <label>make forge smush</label>
    <description>Make forge smush from stoutcap and bull's horns, and a random chunk.</description>
    <jobString>Making wort.</jobString>
    <workSpeedStat>DrugCookingSpeed</workSpeedStat>
    <workSkill>Cooking</workSkill>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Brewing</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <workAmount>1000</workAmount>
    <targetCountAdjustment>1</targetCountAdjustment>
    <researchPrerequisite>SD_ProcessedDrugs</researchPrerequisite>
    <recipeUsers>
      <li>ElectricSmelter</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>SD_RawBullHorns</li>
          </thingDefs>
        </filter>
        <count>15</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>SD_RawStoutcap</li>
          </thingDefs>
        </filter>
        <count>15</count>
      </li>
      <li>
        <filter>
          <categories>
            <li>StoneChunks</li>
          </categories>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>SD_RawStoutcap</li>
        <li>SD_RawBullHorns</li>
      </thingDefs>
      <categories>
        <li>StoneChunks</li>
      </categories>
    </fixedIngredientFilter>
    <products>
      <SD_LiquorSmush>5</SD_LiquorSmush>
    </products>
  </RecipeDef>

