    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Brewing</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <workAmount>1000</workAmount>
    <targetCountAdjustment>1</targetCountAdjustment>
    <researchPrerequisite>SD_ProcessedDrugs</researchPrerequisite>
    <recipeUsers>
      <li>Brewery</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>SD_RawBullHorns</li>
          </thingDefs>
        </filter>
        <count>15</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>SD_RawBladeroot</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>SD_RawStoutcap</li>
        <li>SD_RawBladeroot</li>
      </thingDefs>
      <categories>
        <li>StoneChunks</li>
      </categories>
    </fixedIngredientFilter>
    <products>
      <SD_ElixirMush>5</SD_ElixirMush>
    </products>
  </RecipeDef>
</Defs>
